      "view/title": [
        {
          "command": "kube9.refreshClusters",
          "when": "view == kube9ClusterView",
          "group": "navigation"
        }
      ],
      "view/item/context": [
        {
          "command": "kube9.switchContext",
          "when": "view == kube9ClusterView && viewItem == cluster:inactive",
          "group": "cluster@1"
        },
        {
          "command": "kube9.setActiveNamespace",
          "when": "view == kube9ClusterView && viewItem == namespace:inactive",
          "group": "namespace@1"
        },
        {
          "command": "kube9.clearActiveNamespace",
          "when": "view == kube9ClusterView && viewItem == namespace:active",
          "group": "namespace@1"
        },
        {
          "command": "kube9.describeResource",
          "when": "view == kube9ClusterView && viewItem =~ /^resource/",
          "group": "kube9@1"
        },
        {
          "command": "kube9.describeResourceRaw",
          "when": "view == kube9ClusterView && viewItem =~ /^resource/",
          "group": "kube9@1.5"
        },
        {
          "command": "kube9.viewResourceYAML",
          "when": "view == kube9ClusterView && viewItem =~ /^resource/",
          "group": "kube9@2"
        },
        {
          "command": "kube9.describeNodeRaw",
          "when": "view == kube9ClusterView && viewItem == resource:Node",
          "group": "kube9@2"
        },
        {
          "command": "kube9.openTerminal",
          "when": "view == kube9ClusterView && viewItem == resource:Pod",
          "group": "kube9@2"
        },
        {
          "command": "kube9.viewPodLogs",
          "when": "view == kube9ClusterView && viewItem == resource:Pod",
          "group": "kube9@2"
        },
        {
          "command": "kube9.scaleWorkload",
          "when": "view == kube9ClusterView && (viewItem == resource:Deployment || viewItem == resource:StatefulSet || viewItem == resource:ReplicaSet)",
          "group": "kube9@3"
        },
        {
          "command": "kube9.restartWorkload",
          "when": "view == kube9ClusterView && (viewItem == resource:Deployment || viewItem == resource:StatefulSet || viewItem == resource:DaemonSet)",
          "group": "kube9@3"
        },
        {
          "command": "kube9.deleteResource",
          "when": "view == kube9ClusterView && viewItem =~ /^resource/",
          "group": "kube9@3"
        },
